# Global settings
CONTAINER_GROUP_NAME=AIKit

# PostgreSQL settings
POSTGRES_USER=root
POSTGRES_PASSWORD=password
POSTGRES_DB=n8n
DB_POSTGRESDB_HOST=postgres
DB_POSTGRESDB_USER=${POSTGRES_USER}
DB_POSTGRESDB_PASSWORD=${POSTGRES_PASSWORD}

OLLAMA_HOST=host.docker.internal:11434
# Required for n8n
N8N_ENCRYPTION_KEY=123456789012345678901234
N8N_USER_MANAGEMENT_JWT_SECRET=123456789012345678901234
N8N_ENFORCE_SETTINGS_FILE_PERMISSIONS=true
N8N_DIAGNOSTICS_ENABLED=false
N8N_PERSONALIZATION_ENABLED=false
N8N_COMMUNITY_PACKAGES_ALLOW_TOOL_USAGE=true

# WebUI settings
USER_AGENT=n8n-starter
WEBUI_SECRET_KEY=your-secret-key-here

# Database settings for n8n
DB_TYPE=postgresdb
DB_HOST=postgres
DB_POSTGRESDB_DATABASE=n8n

# Jupyter settings
JUPYTER_TOKEN=your-secure-token-here

# GitIngest settings
GITHUB_TOKEN=your-github-token-here